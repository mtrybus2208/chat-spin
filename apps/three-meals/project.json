{
  "name": "three-meals",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/three-meals/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/three-meals",
        "browser": "apps/three-meals/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/three-meals/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/three-meals/src/assets",
          {
            "glob": "**/*",
            "input": "apps/three-meals/public",
            "output": "assets"
          }
        ],
        "styles": ["apps/three-meals/src/styles.scss"],
        "stylePreprocessorOptions": {
          "includePaths": ["libs"]
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "fileReplacements": [
        {
          "replace": "apps/three-meals/src/environments/environment.ts",
          "with": "apps/three-meals/src/environments/environment.prod.ts"
        }
      ],
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular/build:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "three-meals:build:production"
        },
        "development": {
          "buildTarget": "three-meals:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular/build:extract-i18n",
      "options": {
        "buildTarget": "three-meals:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/three-meals/jest.config.ts",
        "tsConfig": "apps/three-meals/tsconfig.spec.json"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "three-meals:build",
        "port": 4200,
        "staticFilePath": "dist/apps/three-meals/browser",
        "spa": true
      }
    }
  }
}
